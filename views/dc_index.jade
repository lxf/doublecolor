extends layout
block content
	div#container.container
		for item in data
			div.panel.panel-primary
				div.panel-heading
					h3.panel-title
						| #{item.no} #{item.date}
				div.panel-body
					div.row.index_body
						span.col-sm-1 红球1
						span.col-sm-1 红球2
						span.col-sm-1 红球3
						span.col-sm-1 红球4
						span.col-sm-1 红球5
						span.col-sm-1 红球6
						span.col-sm-1 篮球1
					div.row.index_body
						span.col-sm-1.r 
							.a #{item.r1}
						span.col-sm-1.r 
							.a #{item.r2}
						span.col-sm-1.r 
							.a #{item.r3}
						span.col-sm-1.r 
							.a #{item.r4}
						span.col-sm-1.r 
							.a #{item.r5}
						span.col-sm-1.r 
							.a #{item.r6}
						span.col-sm-1.b 
							.a #{item.b1}
	include footer.jade